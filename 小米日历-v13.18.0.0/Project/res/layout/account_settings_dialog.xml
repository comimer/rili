        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <TextView android:textSize="@dimen/font_size_50" android:id="@id/source_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/card_title_margin_start" android:text="@string/calendar_import_source" android:fontFamily="miui-bold" />
            <TextView android:textSize="@dimen/match_score_text_extra" android:textColor="@color/travel_list_secondary_text_color" android:id="@id/path_source" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/ticket_activity_card_date_margin_end" />
        </LinearLayout>
    </com.android.calendar.common.MaxHeightScrollView>
</LinearLayout>